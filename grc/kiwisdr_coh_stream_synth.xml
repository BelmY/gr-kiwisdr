<?xml version="1.0"?>
<block>
  <name>coh_stream_synth</name>
  <key>kiwisdr_coh_stream_synth</key>
  <category>[kiwisdr]</category>
  <import>import kiwisdr</import>
  <make>kiwisdr.coh_stream_synth($num_streams, $fs_in, $df_in)</make>
  <param>
    <name>fs_in</name>
    <key>fs_in</key>
    <value>12000</value>
    <type>int</type>
  </param>
  <param>
    <name>df_in</name>
    <key>df_in</key>
    <value>8000</value>
    <type>int</type>
  </param>
  <param>
    <name>num_streams</name>
    <key>num_streams</key>
    <value>3</value>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>$num_streams</nports>
  </sink>

  <source>
    <name>out</name>
    <type>complex</type>
    <nports>1</nports>
  </source>
  <source>
    <name>resamp</name>
    <type>complex</type>
    <nports>$num_streams</nports>
    <optional>1</optional>
  </source>
  <source>
    <name>phase_qa</name>
    <type>complex</type>
    <nports>$num_streams-1</nports>
    <optional>1</optional>
  </source>
</block>
